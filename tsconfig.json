{
  "extends": "@tsconfig/node16",
  "include": ["src/**/*"],
  "compilerOptions": {
    "outDir": "dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "declaration": true,
    "noImplicitAny": true
  }
}
